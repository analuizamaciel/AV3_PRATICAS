#include "DHT.h"

int dhtPin = 15;
DHT dht(dhtPin, DHT22);

void setup() {
  Serial.begin(115200);
  dht.begin();
}

void loop() {
  float t = dht.readTemperature();
  float h = dht.readHumidity();

  Serial.print("Temp: ");
  Serial.print(t);
  Serial.print("  |  Umid: ");
  Serial.println(h);

  delay(1000);
}
